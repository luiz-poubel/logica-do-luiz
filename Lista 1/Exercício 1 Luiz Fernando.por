programa
{
	
	funcao inicio()
	{
		inteiro a, b, soma 
	     escreva("Digite o primeiro numero:")
	     leia(a)
	     escreva("Digite o segundo número:")
	     leia(b)

	     soma = a + b 
	     escreva("\nA soma dos números é igual a: ", soma)
	
	}
}
